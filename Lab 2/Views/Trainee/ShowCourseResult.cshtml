@using Lab_2.ViewModel;
@model List<TraineeCrsColorDegree>
@{
    ViewData["Title"] = "ShowCourseResult";
}
<h1>Course Result </h1>
<table class="table table-border">
   <tr>
       <th>Name</th>
        <th>Degree</th>
   </tr>
    @foreach(var item in Model)
    {
        <tr style="color:@item.color">
            <th>@item.trn_Name</th>
            <th>@item.degree</th>
        </tr>
    }



</table>

